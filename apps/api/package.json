    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/passport-jwt": "^4.0.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "ioredis-mock": "^8.13.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0",
    "prisma": "^6.19.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
